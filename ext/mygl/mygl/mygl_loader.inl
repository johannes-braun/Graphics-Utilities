#else
#if defined __CYGWIN__
    constexpr static std::array<const char *, 3> libs = {
        "libGL-1.so",
#else
    constexpr static std::array<const char *, 2> libs = {
#endif
        "libGL.so.1",
        "libGL.so"
    };
#endif
};

void* __get_mygl_func(const char* name)
{
    static function_loader fl;
    return fl.get(name);
}
